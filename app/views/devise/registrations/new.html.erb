<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
<%= devise_error_messages! %>

<div class="login form-group">

  <div class="col-md-12">
    <%= f.label :Nombre %><br/>
    <%= f.text_field :name, autofocus: true, class:'col-md-12 form-control' %>
  </div>

  <div class="col-md-12">
    <%= f.label :email %><br/>
    <%= f.email_field :email, class:'col-md-12 form-control'%>
  </div>

  <div class="col-md-12">
    <%= f.label :Contraseña %><br/>
    <%= f.password_field :password, autocomplete: "off", class:'col-md-12 form-control' %>
  </div>

  <div class="col-md-12">
    <%= f.label "Confirmación de Contraseña " %><br/>
    <%= f.password_field :password_confirmation, autocomplete: "off", class:'col-md-12 form-control' %>
  </div>

  <div class="col-md-12">
    <button type="submit" value="Send" id="submit" class="btn btn-info col-md-12 mt-2 mb-4">Registrar</button>

  </div>
<% end %>
<div class="text-center">
  <%= render "devise/shared/links" %>
</div>
</div>
